generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  password    String
  credentials credentials[]
  notes       notes[]
}

model credentials {
  id       Int    @id @default(autoincrement())
  userId   Int
  name     String
  password String
  title    String
  url      String
  users    users  @relation(fields: [userId], references: [id])
}

model notes {
  id     Int    @id @default(autoincrement())
  userId Int
  title  String
  text   String
  users  users  @relation(fields: [userId], references: [id])
}
